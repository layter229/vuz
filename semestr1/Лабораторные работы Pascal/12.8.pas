uses graphABC;
var r,k,i,g: integer;
begin
  r:=90;
  setwindowsize(800,300);
  setpencolor(clBlack);
  k:=0;
  g:=36;
  
  for i:=1 to 2 do
  begin
    setbrushstyle(bshatch);
    setbrushhatch(bhDashedUpwardDiagonal);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(150,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(bshatch);
    setbrushhatch(bhDiagonalCross);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(150,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(Bshatch);
    setbrushhatch(bhLargeConfetti);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(150,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(bshatch);
    setbrushhatch(bhDottedDiamond);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(150,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(bshatch);
    setbrushhatch(bhLargeCheckerBoard);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(150,150,r,g,k);
    k:=k+36;
    g:=g+36;
  end;
  k:=0;
  g:=36;
  
  for i:=1 to 2 do
  begin
    setbrushstyle(bshatch);
    setbrushhatch(bhSolidDiamond);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(400,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(bshatch);
    setbrushhatch(bhMax);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(400,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(Bshatch);
    setbrushhatch(bhSmallCheckerBoard);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(400,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(bshatch);
    setbrushhatch(bhHorizontalBrick);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(400,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(bshatch);
    setbrushhatch(bhDashedHorizontal);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(400,150,r,g,k);
    k:=k+36;
    g:=g+36;
  end;
  
  for i:=1 to 2 do
  begin
    setbrushstyle(bshatch);
    setbrushhatch(bhDashedUpwardDiagonal);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(650,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(bshatch);
    setbrushhatch(bhDarkHorizontal);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(650,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(Bshatch);
    setbrushhatch(bhNarrowVertical);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(650,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(bshatch);
    setbrushhatch(bhWideDownwardDiagonal);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(650,150,r,g,k);
    k:=k+36;
    g:=g+36;
    setbrushstyle(bshatch);
    setbrushhatch(bhPercent60);
    setbrushcolor(rgb(random(256),random(256),random(256)));
    pie(650,150,r,g,k);
    k:=k+36;
    g:=g+36;
    end;
end.